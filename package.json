{
  "name": "js-intro-workshop",
  "version": "1.0.0",
  "description": "Code used in the Introduction to JavaScript course",
  "main": "index.html",
  "scripts": {
    "client": "serve",
    "server": "nodemon --inspect -w ./server ./server/server.js",
    "start": "concurrently -k \"npm run client\" \"npm run server\"",
    "build": "tsc -p ./client/tsconfig.json"
  },
  "repository": {
