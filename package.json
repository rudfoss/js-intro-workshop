    "url": "git+https://github.com/rudfoss/js-intro-workshop.git"
  },
  "keywords": [
    "course",
    "js",
    "intro",
    "javascript",
    "vanillajs"
  ],
  "author": "Thomas Haugland Rudfoss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudfoss/js-intro-workshop/issues"
  },
  "homepage": "https://github.com/rudfoss/js-intro-workshop#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "serve": "^10.1.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4"
  }
}
